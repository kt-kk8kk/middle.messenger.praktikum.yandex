{
    "extends": "airbnb",
    "env": {
        "es6": true
    },
    "rules": {
        "no-var": "error",
        "semi": "error",
        "indent": "error",
        "no-multi-spaces": "error",
        "space-in-parens": "error",
        "no-multiple-em pty-lines": "error",
        "prefer-const": "error",
        "no-use-before-define": "error",
        "max-len": [2, 100],
        "max-params": [2, 3],
        "eol-last": ["error", "always"]
    }
}
